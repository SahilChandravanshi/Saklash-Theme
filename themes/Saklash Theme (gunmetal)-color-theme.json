    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#c678dd",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
