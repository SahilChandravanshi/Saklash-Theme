      "settings": {
        "foreground": "#24b5a8"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#37ae6f"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#24b5a8"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.variable"
      ],
      "settings": {
        "foreground": "#24b5a8"
      }
    },
    {
      "name": "Json Property",
