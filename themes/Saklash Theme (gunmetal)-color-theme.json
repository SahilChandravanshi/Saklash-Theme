    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#738dfd"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter",
        "entity.name.variable.parameter",
        "parameter.variable"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#abb2bf"
      }
    },
    {
      "name": "Builtin Functions",
      "scope": [
        "function.support.builtin",
        "function.support.core"
      ],
      "settings": {
        "foreground": "#37ae6f"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.class.js"
